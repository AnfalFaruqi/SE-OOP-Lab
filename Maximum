package maximum;
class Maximum
{
    public static void main(String args[])
    {
        int a=15;
        int b=10;
        int c=20;
        
        if(a>b)
        
            if(a>c)
                System.out.println(a + " is the biggest.");
        
        else if(b>c)
        
            System.out.println(b + " is the biggest.");
        
        else
        System.out.println(c + " is the biggest.");
    } 
}
